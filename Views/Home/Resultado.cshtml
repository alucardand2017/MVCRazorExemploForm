@model IEnumerable<Resposta>

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/lib/bootstrap5/dist/css/bootstrap.css">
  <title>Enquete Web</title>
</head>
<body>
  <h1>Resultado Parcial</h1>
  <h2>Pergunta: Você iria para a lua?</h2>
  @{
    foreach(Resposta resposta in Model)
    {
        <p>@(resposta.Nome): @(resposta.Escolha.Value ? "Sim" : "Não")</p>
    }
  }
  <a asp-action="Index" class="btn btn-primary">Voltar</a>
</body>
</html>